[
    {
      "key": "ctrl+shift+j",
      "command": "workbench.action.terminal.focus",
      "when": "editorFocus"
    },
    {
      "key": "ctrl+shift+k",
      "command": "workbench.action.focusActiveEditorGroup",
      "when": "terminalFocus"
    },
    {
      "key": "ctrl+shift+h",
      "command": "workbench.view.explorer",
      "when": "editorFocus"
    },
    {
      "key": "ctrl+shift+l",
      "command": "workbench.action.focusActiveEditorGroup",
      "when": "focusedView != ''"
    }
]
